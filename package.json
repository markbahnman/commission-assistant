{
  "name": "commission-assistant",
  "version": "1.0.0",
  "description": "Commission Assistant",
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "postinstall": "webpack --display-error-details --config webpack/prod.config.js",
    "prestart-prod-api": "webpack --display-error-details --config webpack/prod.config.js",
    "preebs": "webpack --display-error-details --config webpack/prod.config.js",
    "ebs": "better-npm-run ebs",
    "lint": "eslint -c .eslintrc src api",
    "lint-watch": "esw -c .eslintrc src api --watch",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "karma start",
    "test-node": "better-npm-run test-api",
    "test-api": "better-npm-run unit-test-api",
    "integration-test-api": "better-npm-run integration-test-api"
  },
  "betterScripts": {
    "test-api": {
      "command": "./node_modules/mocha/bin/mocha ./api/__tests__/**/*-test.js --compilers js:babel-core/register",
      "env": {
        "NODE_ENV": "testing",
        "NODE_PATH": "./api",
        "APIPORT": 8081
      }
    },
    "integration-test-api": {
      "command": "./node_modules/mocha/bin/mocha ./api/**/__tests__/integration/*-test.js --compilers js:babel-core/register",
      "env": {
        "NODE_ENV": "testing",
        "NODE_PATH": "./api",
        "APIPORT": 8081
      }
    },
    "unit-test-api": {
      "command": "./node_modules/mocha/bin/mocha ./api/**/__tests__/unit/*-test.js --compilers js:babel-core/register",
      "env": {
        "NODE_ENV": "testing",
        "NODE_PATH": "./api",
        "APIPORT": 8081
      }
    },
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "ebs": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "TYPE": "WEB",
        "APIHOST": "commissionassistantapi.elasticbeanstalk.com",
        "APIPORT": 80
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "TYPE": "API",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "POSTGRES": "postgres://mark:bahnman@localhost/commission-assistant",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "author": "@markbahnman",
  "license": "ISC",
  "dependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "~5.8.29",
    "babel-core": "~5.8.33",
    "babel-loader": "~5.3.3",
    "babel-plugin-react-transform": "~1.1.1",
    "babel-plugin-typecheck": "^2.0.0",
    "babel-runtime": "~5.8.29",
    "better-npm-run": "^0.0.4",
    "body-parser": "^1.14.1",
    "clean-webpack-plugin": "^0.1.6",
    "compression": "^1.6.0",
    "connect-redis": "^3.0.1",
    "cors": "^2.7.1",
    "credential": "^0.2.6",
    "css-loader": "^0.23.1",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "helmet": "^0.15.0",
    "history": "2.0.0-rc2",
    "http-proxy": "^1.12.0",
    "invariant": "^2.2.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "lru-memoize": "^1.0.0",
    "multireducer": "^2.0.0",
    "newrelic": "^1.24.0",
    "node-sass": "^3.4.2",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2",
    "pg-native": "^1.9.0",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "react": "^0.14.2",
    "react-helmet": "^2.2.0",
    "react-dom": "^0.14.1",
    "react-inline-css": "^2.0.0",
    "react-redux": "^4.0.0",
    "react-router": "2.0.0-rc5",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.4",
    "redux-async-connect": "^0.1.10",
    "redux-immutable-state-invariant": "^1.2.0",
    "react-router-redux": "^2.1.0",
    "sass-loader": "^3.1.2",
    "sequelize": "^3.14.2",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "superagent": "^1.4.0",
    "susy": "^2.2.9",
    "url-loader": "^0.5.7",
    "validator": "^4.3.0",
    "warning": "^2.1.0",
    "webpack": "^1.12.9",
    "webpack-isomorphic-tools": "^2.2.18"
  },
  "devDependencies": {
    "async": "^1.5.0",
    "babel-eslint": "^4.1.3",
    "bluebird": "^3.0.5",
    "chai": "^3.3.0",
    "concurrently": "^0.1.1",
    "eslint": "1.8.0",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "eslint-watch": "^2.1.4",
    "fetch-mock": "^4.0.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.10",
    "karma-cli": "^0.1.1",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "nock": "^4.0.0",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-mock-store": "0.0.6",
    "supertest": "^1.1.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0"
  },
  "engines": {
    "node": "5.0.0"
  }
}
